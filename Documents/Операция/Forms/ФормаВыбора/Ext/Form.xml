			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументОперация.Ссылка,
	ДокументОперация.Дата,
	ДокументОперация.ТипОперации,
	ДокументОперация.Кошелек,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.перевод)
			ТОГДА ДокументОперация.КошелекПриемник
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.Долги)
			ТОГДА ДокументОперация.ФизЛицо
		ИНАЧЕ ДокументОперация.Статья
	КОНЕЦ КАК Статья,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.расход)
			ТОГДА -ДокументОперация.Сумма
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.Долги)
			ТОГДА ВЫБОР
					КОГДА ДокументОперация.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.дебет)
						ТОГДА -ДокументОперация.Сумма
					ИНАЧЕ ДокументОперация.Сумма
				КОНЕЦ
		ИНАЧЕ ДокументОперация.Сумма
	КОНЕЦ КАК Сумма,
	ДокументОперация.Комментарий,
	ДокументОперация.КошелекПриемник,
	ДокументОперация.ФизЛицо,
	ДокументОперация.Метка,
	ДокументОперация.Кошелек.Валюта КАК Валюта,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.расход)
			ТОГДА -ДокументОперация.СуммаОбщиеЗатраты
		ИНАЧЕ ДокументОперация.СуммаОбщиеЗатраты
	КОНЕЦ КАК СуммаОбщиеЗатраты
ИЗ
	Документ.Операция КАК ДокументОперация</QueryText>
