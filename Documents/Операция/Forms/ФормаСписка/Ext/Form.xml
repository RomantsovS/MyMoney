	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>~Список.БезКошелька</Field>
				<Field>Список.Валюта</Field>
				<Field>Список.Кошелек</Field>
				<Field>Список.КошелекПриемник</Field>
				<Field>Список.Ссылка</Field>
				<Field>Список.ТипОперации</Field>
				<Field>Список.ФизЛицо</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ДокументОперация.Ссылка КАК Ссылка,
	ДокументОперация.Дата КАК Дата,
	ДокументОперация.Дата КАК Дата1,
	ДокументОперация.ТипОперации КАК ТипОперации,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.Долги)
				И (ДокументОперация.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.Списание)
					ИЛИ ДокументОперация.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.Зачисление))
			ТОГДА ДокументОперация.ФизЛицо
		ИНАЧЕ ДокументОперация.Кошелек
	КОНЕЦ КАК Кошелек,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.перевод)
			ТОГДА ДокументОперация.КошелекПриемник
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.Долги)
			ТОГДА ВЫБОР
					КОГДА ДокументОперация.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.Списание)
							ИЛИ ДокументОперация.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.Зачисление)
						ТОГДА ДокументОперация.Статья
					ИНАЧЕ ДокументОперация.ФизЛицо
				КОНЕЦ
		ИНАЧЕ ДокументОперация.Статья
	КОНЕЦ КАК Статья,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(Перечисление.ТипыОпераций.Сторно)
			ТОГДА ВЫБОР
					КОГДА ДокументОперация.Основание.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.расход)
						ТОГДА ДокументОперация.Сумма
					КОГДА ДокументОперация.Основание.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.Долги)
						ТОГДА ВЫБОР
								КОГДА ДокументОперация.Основание.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.дебет)
									ТОГДА ДокументОперация.Сумма
								ИНАЧЕ -ДокументОперация.Сумма
							КОНЕЦ
					ИНАЧЕ -ДокументОперация.Сумма
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.расход)
					ТОГДА -ДокументОперация.Сумма
				КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.Долги)
					ТОГДА ВЫБОР
							КОГДА ДокументОперация.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.дебет)
								ТОГДА -ДокументОперация.Сумма
							ИНАЧЕ ДокументОперация.Сумма
						КОНЕЦ
				КОГДА ДокументОперация.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.Взаимозачет)
					ТОГДА "-"
				ИНАЧЕ ДокументОперация.Сумма
			КОНЕЦ
	КОНЕЦ КАК Сумма,
	ДокументОперация.Комментарий КАК Комментарий,
	ДокументОперация.КошелекПриемник КАК КошелекПриемник,
	ДокументОперация.ФизЛицо КАК ФизЛицо,
	ДокументОперация.Метка КАК Метка,
	ДокументОперация.Валюта КАК Валюта,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(Перечисление.ТипыОпераций.Сторно)
			ТОГДА ВЫБОР
					КОГДА ДокументОперация.Основание.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.расход)
						ТОГДА ДокументОперация.СуммаОбщиеЗатраты
					ИНАЧЕ -ДокументОперация.СуммаОбщиеЗатраты
				КОНЕЦ
		ИНАЧЕ ВЫБОР
				КОГДА ДокументОперация.Основание.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.расход)
					ТОГДА -ДокументОперация.СуммаОбщиеЗатраты
				ИНАЧЕ ДокументОперация.СуммаОбщиеЗатраты
			КОНЕЦ
	КОНЕЦ КАК СуммаДоп1,
	ВЫБОР
		КОГДА ДокументОперация.ТипОперации = ЗНАЧЕНИЕ(Перечисление.ТипыОпераций.Сторно)
			ТОГДА ВЫБОР
					КОГДА ДокументОперация.Основание.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.расход)
						ТОГДА ВЫБОР
								КОГДА ДокументОперация.СуммаОбщиеЗатраты - ДокументОперация.Сумма &gt;= ДокументОперация.Сумма
									ТОГДА ДокументОперация.СуммаДолг
								ИНАЧЕ -ДокументОперация.СуммаДолг
							КОНЕЦ
					КОГДА ДокументОперация.Основание.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыОпераций.доход)
						ТОГДА ДокументОперация.СуммаДолг
					КОГДА ДокументОперация.Основание.ТипОперации = ЗНАЧЕНИЕ(перечисление.типыопераций.долги)
						ТОГДА ВЫБОР
								КОГДА ДокументОперация.Основание.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.Зачисление)
									ТОГДА -ДокументОперация.Сумма
								КОГДА ДокументОперация.Основание.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.Списание)
									ТОГДА ДокументОперация.Сумма
								КОГДА ДокументОперация.СуммаДолг = 0
									ТОГДА ВЫБОР
											КОГДА ДокументОперация.Основание.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.дебет)
												ТОГДА -ДокументОперация.Сумма
											ИНАЧЕ ДокументОперация.Сумма
										КОНЕЦ
								ИНАЧЕ ВЫБОР
										КОГДА ДокументОперация.Основание.ТипДолга = ЗНАЧЕНИЕ(перечисление.ТипыДолгов.дебет)
